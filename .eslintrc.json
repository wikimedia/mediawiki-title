{
	"extends": "eslint-config-node-services",
	"rules": {
		"computed-property-spacing": 0,
		"consistent-this": 0,
		"func-style": 0,
		"guard-for-in": 0,
		"indent": 0,
		"max-len": 0,
		"multiline-ternary": 0,
		"no-extra-boolean-cast": 0,
		"no-loop-func": 0,
		"no-nested-ternary": 0,
		"no-tabs": 0,
		"no-undef": 0,
		"no-unused-vars": 0,
		"no-var": 0,
		"object-shorthand": 0,
		"prefer-arrow-callback": 0,
		"prefer-template": 0,
		"semi": 0,
		"space-before-function-paren": 0,
		"space-in-parens": 0,
		"strict": 0,
		"vars-on-top": 0,
		"wrap-iife": 0,
		"jsdoc/check-tag-names": 0,
		"jsdoc/newline-after-description": 0
	}
}
